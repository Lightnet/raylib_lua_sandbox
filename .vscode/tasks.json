{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  // https://love2d.org/wiki/love.load
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Raylib Lua bat",
      "type": "shell",
      "command": "${workspaceRoot}\\run.bat",
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Raylib Lua exe",
      "type": "shell",
      "command": "D:/dev/raylib_lua/raylua_s.exe",
      "args": [
        "main.lua"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}